# MCPB Ignore File
# Files and directories to exclude from the MCPB bundle

# Source files (only dist is needed)
src/
tsconfig.json
*.ts

# Development files
.git/
.github/
.vscode/
.claude/
.ascension/
.smithery/
coverage/
tests/
*.spec.ts
*.test.ts
jest.config.js

# Build artifacts
*.tsbuildinfo

# Documentation (README is included via manifest)
docs/
*.md
!README.md

# Package management
package-lock.json
yarn.lock
.npmrc
.npmignore
.nvmrc

# Environment and secrets
.env*
*.token
.mcpregistry*

# Docker (not needed for MCPB)
Dockerfile
docker-compose*.yml
.dockerignore

# Smithery config (separate from MCPB)
smithery.yaml

# Storage and cache (runtime generated)
storage/
.cache/

# Misc
*.log
*.tmp
.DS_Store
Thumbs.db
*.roomodes
server.json

# Large dev dependencies in node_modules
node_modules/.cache/
node_modules/@types/
node_modules/typescript/
node_modules/prettier/
node_modules/eslint*/
node_modules/jest*/
node_modules/@jest/
node_modules/ts-jest/
node_modules/@playwright/test/
node_modules/.bin/
node_modules/**/test/
node_modules/**/tests/
node_modules/**/*.d.ts
node_modules/**/*.map
node_modules/**/docs/
node_modules/**/example/
node_modules/**/examples/
node_modules/**/.github/

# Test fixtures
**/fixtures/
**/test-data/

# Output files
*.mcpb
